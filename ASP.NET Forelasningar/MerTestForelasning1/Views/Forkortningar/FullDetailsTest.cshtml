@model IEnumerable<MerTestForelasning1.Models.ForkortningarModel>

@{
    ViewBag.Title = "FullDetailsTest";
}

<h2>FullDetailsTest</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Abbreviation)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Explanation)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @item.Abbreviation
            </td>
            <td>
                @item.Explanation
            </td>
        </tr>
    }

</table>